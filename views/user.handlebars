{{> navbar user}}


<div class="container">
    <div class="row">
        <div class="col-md">
            <div class="jumbotron">
                <h4 class="display-5 text-center" >Hello, {{user.firstname}}!</h4>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md">
           
            <div id="users">
              
                <div class="card mb-3">
                   
                    <div class="card-body">
                        <p class="card-text">Contact Number: {{user.phone}}</p>
                        <p class="card-text">Email : {{user.email}}</p>
                        <p class="card-text">Age : {{user.age}}</p>
                        <p class="card-text">Gender: {{user.gender}}</p>
                    
                        <br>
                        @ {{user.createdAt}}
                    </div>

                </div>
               
            </div>
        </div>


    </div>
</div>